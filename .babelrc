{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react",
        "stage-2"
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
  
}